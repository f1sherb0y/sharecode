# Frontend Environment Defaults
# Copy this file to `.env` and adjust as needed.

# API endpoints (leave unset to use same-origin URLs)
VITE_API_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001/api/ws

# Toggle user registration UI (set to "false" to hide the register form)
VITE_ALLOW_REGISTRATION=false
